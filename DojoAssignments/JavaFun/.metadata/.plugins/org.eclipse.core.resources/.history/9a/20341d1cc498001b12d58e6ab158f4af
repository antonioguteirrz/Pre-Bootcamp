package zooKeepers;

class Bat extends Mammal{
		this.energyLevel =300;
	public void fly() {
		System.out.println("Wee, I fly to the sky");
		energyLevel =-50;
	}
	public void eatHumans() {
		energyLevel =+25;
	}
	public void attackTown() {
		System.out.print("Oh no, car fire,everything fire,me fire");
		energyLevel =-100;
	}
}
